{
  "name": "jacopo/laravel-single-table-inheritance",
  "description": "A package that contains a model for single table inheritance with eloquent(laravel orm)",
  "version": "0.1.0",
  "keywords": ["single", "table", "inheritance", "laravel", "eloquent", "model"],
  "license": "MIT",
  "authors": [
    {
      "name": "Jacopo Beschi",
      "email": "beschi.jacopo@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "illuminate/support": "~4"
  },
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "illuminate/database": "~4",
    "orchestra/testbench": "2.2.*"
  },
  "autoload": {
    "classmap": [
      "src/migrations"
    ],
    "psr-4": {
      "Jacopo\\LaravelSingleTableInheritance\\": "src/Jacopo/LaravelSingleTableInheritance",
      "Jacopo\\LaravelSingleTableInheritance\\Tests\\" : "tests"
    }
  },
  "minimum-stability": "stable"
}

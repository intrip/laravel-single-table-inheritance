    "version" : "0.1.0",
    "keywords": ["single", "table", "inheritance", "laravel", "eloquent", "model"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jacopo Beschi",
            "email": "beschi.jacopo@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.0.x"
    },
    "require-dev": {
        "mockery/mockery": "dev-master@dev",
        "illuminate/database": "4.0.x"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
